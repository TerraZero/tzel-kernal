{
  "logger": {
    "log": [
      {
        "type": "console"
      }
    ],
    "warn": [
      {
        "type": "console"
      }
    ],
    "error": [
      {
        "type": "console"
      }
    ],
    "debug": [
      {
        "type": "console",
        "head": "$time [$mod]: "
      }
    ]
  },
  "modules": {
    "enabled": [
      "tzel-core",
      "tzel-cli"
    ]
  },
  "path": {
    "files": "files",
    "tmp": "files/tmp",
    "configs": "files/configs",
    "tpls": "files/tpls"
  }
}
